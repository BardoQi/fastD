{
    "name": "fastd/fastd",
    "type": "library",
    "license": "MIT",
    "description": "FastD",
    "minimum-stability": "stable",
    "keywords": ["FastD"],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/src"
        },
        "files": [
            "tests/TestCase.php"
        ]
    },
    "require": {
        "php": ">=5.6",
        "fastd/container": "~3.0.0",
        "fastd/http": "~3.0.0",
        "fastd/middleware": "~1.0.0",
        "fastd/basic-authenticate": "~0.1.0",
        "fastd/config": "3.1.x-dev",
        "fastd/routing": "~3.0.0",
        "fastd/swoole": "2.0.x-dev",
        "fastd/utils": "0.1.x-dev",
        "fastd/testing": "0.2.x-dev",
        "catfan/Medoo": "~1.0",
        "symfony/cache": "~3.2",
        "monolog/monolog": "~1.0",
        "robmorgan/phinx": "^0.8.0",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "phpunit/dbunit": "^2.0",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "bin": [
        "console",
        "server"
    ]
}
