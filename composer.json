        "fastd/basic-authenticate": "~0.1.0",
        "fastd/config": "^3.1@dev",
        "fastd/routing": "~3.0.0",
        "fastd/swoole": "^2.0@dev",
        "fastd/utils": "^0.1@dev",
        "fastd/testing": "^0.2@dev",
        "catfan/Medoo": "~1.0",
        "symfony/cache": "~3.2",
        "monolog/monolog": "~1.0",
        "robmorgan/phinx": "^0.8.0"
    },
    "require-dev": {
        "phpunit/dbunit": "^2.0"
