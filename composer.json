{
    "name": "fastd/fastd",
    "type": "library",
    "license": "MIT",
    "description": "FastD",
    "minimum-stability": "stable",
    "keywords": [
        "FastD"
    ],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/app/src"
        }
    },
    "require": {
        "php": ">=5.6",
        "fastd/container": "dev-develop",
        "fastd/testing": "dev-develop",
        "fastd/config": "dev-develop",
        "fastd/swoole": "dev-develop",
        "fastd/middleware": "dev-develop",
        "fastd/http": "dev-develop",
        "fastd/routing": "dev-develop",
        "fastd/utils": "dev-develop",
        "fastd/migration": "dev-develop",
        "fastd/query-builder": "dev-develop",
        "symfony/cache": "~3.2",
        "monolog/monolog": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "bin": [
        "console",
        "client",
        "server"
    ]
}
