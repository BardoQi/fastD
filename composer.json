{
    "name": "fastd/fastd",
    "type": "library",
    "license": "MIT",
    "description": "The FastD High Performance API Framework",
    "minimum-stability": "stable",
    "keywords": ["Framework", "API", "Swoole", "FastD"],
    "homepage": "https://fastdlabs.com",
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "": "app/src"
        }
    },
    "require": {
        "php": ">=7.1",
        "fastd/container": "^3.0",
        "fastd/http": "^3.0",
        "fastd/middleware": "^1.0",
        "fastd/config": "^3.2",
        "fastd/routing": "^3.0",
        "fastd/swoole": "^2.0",
        "fastd/testing": "^0.3",
        "fastd/event": "^1.0",
        "symfony/cache": "^3.0",
        "monolog/monolog": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/http-foundation": "^4.0"
    },
    "bin": [
        "bin/console",
        "bin/client",
        "bin/server",
        "bin/process"
    ]
}
