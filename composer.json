{
    "name": "fastd/fastd",
    "type": "library",
    "license": "MIT",
    "description": "FastD",
    "minimum-stability": "stable",
    "keywords": ["FastD"],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
