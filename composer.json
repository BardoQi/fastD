{
    "name": "fastd/fastd",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "description": "FastD php simple framework",
    "keyword": [
        "fastd ",
        "fastd php",
        "simple framework",
        "php simple framework",
        "simple php framework"
    ],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "fastd/protocol": "~0.1.0",
        "fastd/routing": "~0.1.0",
        "fastd/database": "~0.1.0",
        "fastd/container": "~0.1.0",
        "fastd/config": "~0.1.0"
    },
    "require-dev": {
        "fastd/storage": "dev-dev",
        "fastd/logger": "dev-dev",
        "fastd/console": "dev-dev",
        "fastd/finder": "dev-dev",
        "fastd/template": "dev-dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "web": "public"
    },
    "autoload": {
        "psr-4": {
            "": "src",
            "Kernel\\": "app/kernel",
            "Helpers\\": "app/helpers"
        }
    }
}
