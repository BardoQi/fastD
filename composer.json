{
    "name": "dobee/dobee-framework",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "description": "php simple framework",
    "keyword": [
        "dobee php",
        "simple framework",
        "php simple framework",
        "simple php framework"
    ],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "dobee/protocol": "~0.1.0",
        "dobee/routing": "~0.1.0",
        "dobee/config": "~0.1.0",
        "dobee/container": "~0.1.0",
        "dobee/database": "~0.1.0",
        "dobee/storage": "~0.1.0",
        "dobee/logger": "~0.1.0",
        "dobee/template": "~0.1.0",
        "dobee/console": "~0.1.0",
        "dobee/finder": "~0.1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "web": "public"
