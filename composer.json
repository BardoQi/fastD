{
    "name": "fastd/fastd",
    "type": "library",
    "license": "MIT",
    "description": "FastD Core",
    "minimum-stability": "stable",
    "keywords": ["FastD"],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "": "tests/app/src"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/console": "^3.2",
        "fastd/container": "~3.0.0@rc",
        "fastd/http": "3.0.x-dev",
        "fastd/config": "~3.0.0@rc",
        "fastd/routing": "3.0.x-dev",
        "fastd/middleware": "~1.0.0@rc"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "bin": [
        "console",
        "server"
    ]
}
