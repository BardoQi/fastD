{
    "name": "janhuang/dobee-php-simple-framework",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "description": "php simple framework",
    "keyword": [
        "dobee php",
        "simple framework",
        "php simple framework",
        "simple php framework"
    ],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "dobee/routing": "1.2.0",
        "dobee/config": "1.1.2",
        "dobee/container": "1.1.0",
        "dobee/template": "1.1.1",
        "dobee/storage": "1.0.1",
        "dobee/database": "1.0.1",
        "dobee/logger": "1.0.0",
        "dobee/http": "1.1.3",
        "dobee/console": "dev-master",
        "dobee/uploader": "dev-master",
        "dobee/server": "dev-master",
        "dobee/finder": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "web": "public"
    },
    "autoload": {
        "psr-4": {
            "": "src",
            "Kernel\\": "app/kernel",
            "Helpers\\": "app/helpers"
        }
    }
}
