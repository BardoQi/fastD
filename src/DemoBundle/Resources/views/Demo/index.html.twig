{% extends "layout/layout.html.twig" %}
{% block welcome %}
    path: {{ path('demo_index') }} <br />
    name: demo_index
    <hr />
{% endblock %}
{% block main %}
    {% for key, value in postRead %}
        {% if key != 'content' %}
        {{ key }} => {{ value }}
        {% endif %}
    {% endfor %}
    <hr />
    {% for key, value in postWrite %}
        {% if key != 'content' %}
            {{ key }} => {{ value }}
        {% endif %}
    {% endfor %}
{% endblock %}