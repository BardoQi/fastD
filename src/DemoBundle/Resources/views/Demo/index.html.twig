{% extends "layout/layout.html.twig" %}

{% block main %}
    {% for key, value in postRead %}
        {% if key != 'content' %}
        {{ key }} => {{ value }}
        {% endif %}
    {% endfor %}
    <hr />
    {% for key, value in postWrite %}
        {% if key != 'content' %}
            {{ key }} => {{ value }}
        {% endif %}
    {% endfor %}
{% endblock %}