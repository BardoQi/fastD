#!/usr/bin/env php
<?php

$application = include __DIR__ . '/../app/bootstrap/bootstrap.php';

use Dobee\Console\Console;
use Dobee\Framework\Commands\Routing\Dump;
use Dobee\Framework\Commands\Compiler\Compile;
use Dobee\Framework\Commands\Config\Caching;

$console = new Console($application);

$console->addCommand(new Dump());
$console->addCommand(new Caching());
$console->addCommand(new Dobee\Framework\Commands\Routing\Caching());
$console->addCommand(new Compile());

$console->run();
